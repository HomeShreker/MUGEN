; Definition file for Kung Fu Man
; Contains all the filenames needed for the character

; Player information
[Info]
name = "Labrys"        ;Name of character
displayname = "Labrys" ;Name of character to display
versiondate = 09,30,2009    ;Version date of character (MM-DD-YYYY or X.XX)
mugenversion = 1.0          ;Version of M.U.G.E.N character works on (X.XX)
author = "HomeShreker"         ;Character author name
pal.defaults = 1,2,3,4      ;Defalult palettes in order of preference (up to 4)
                            ;Numbering starts from 1
localcoord = 320,240        ;Local coordinate space width and height


; Files for the player
[Files]
cmd     = labrys.cmd         ;Command set
cns     = labrys.cns        ;Constants
st      = labrys.cns        ;States
stcommon = common1.cns      ;Common states (from dat
sprite = labrys.sff
anim    = labrys.air        ;Animation
sound   =          			;Sound
ai      =             ;AI hints data (not used)

; Arcade mode
[Arcade]
intro.storyboard = 
ending.storyboard =
